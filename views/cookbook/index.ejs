<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="stylesheets/index/style.css"/>
    <title>Recipes</title>
</head>
<body>
    <div>
        <a id='logout' href="/logout">
            Logout
        </a>
    </div>
    <% recipe.forEach(function(r){ %> 
    
        <ul>
            <h1 id='recipe-title'>
                <%= r.recipe %>
            </h1>

            <a id='recipe-tag' href="/cookbook/<%= r._id %>">
                Recipe
            </a>
        </ul>

    <% }); %> 
</body>
</html>